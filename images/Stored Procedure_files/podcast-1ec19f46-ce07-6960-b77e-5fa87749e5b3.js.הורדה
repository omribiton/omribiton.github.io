    var rwmp_webkit = !!rwmp_ua.match(/WebKit/i);
    var rwmp_safari = !!rwmp_ua.match(/Safari/i);
    var rwmp_CriOS = !!rwmp_ua.match(/CriOS/i);
    var rwmp_iOSSafari = rwmp_iOS && rwmp_webkit && rwmp_safari && !rwmp_CriOS;
