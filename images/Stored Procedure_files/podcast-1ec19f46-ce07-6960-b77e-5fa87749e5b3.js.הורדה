        const urlSearchParams = new URLSearchParams(window.location.search);

    if (urlSearchParams.has('edisound_preroll_preview')) {
        rwmpVars.globalVast = 'https\u003A\/\/publishers.edisound.com\/api\/p\/medias\/vast/0/force_preview/' + urlSearchParams.get('edisound_preroll_preview');
        rwmpLoadPlayer([]);
    }
        else {
        fetch('https\u003A\/\/publishers.edisound.com\/api\/p\/medias\/fetch', {
            method: "POST",
            referrerPolicy: "no-referrer-when-downgrade",
            body: JSON.stringify({uuid: '1ec19f46\u002Dce07\u002D6960\u002Db77e\u002D5fa87749e5b3', user_id: readCookies('_edisound_player'), gdpr_consent: consentString, force_program: urlSearchParams.get('edisound_program_preview'), isFront: false }),
            headers: new Headers({"Content-Type": "text/plain"})
        })
            .then(res => res.json())
            .then((data) => {
                if (data.hasOwnProperty('user_id') && data.user_id != null && readCookies('_edisound_player') == null) {
                    setCookie('_edisound_player', data.user_id, 365);
                }
                else if (readCookies('_edisound_player')) {
                    setCookie('_edisound_player', readCookies('_edisound_player'), 365);
                }
                if (data.hasOwnProperty('yknytmzg') && data.yknytmzg === 1) {
                    rwmpVars.yknytmzg = true;
                }
                if (data.hasOwnProperty('retargetingImpression') && Object.keys(data.retargetingImpression).length !== 0) {
                    rwmpVars.retargetingImpression = data.retargetingImpression;

                    var pxRetargetingImpression = document.createElement("img");
                    pxRetargetingImpression.src = rwmpVars.retargetingImpression.src;
                    pxRetargetingImpression.width = rwmpVars.retargetingImpression.width;
                    pxRetargetingImpression.height = rwmpVars.retargetingImpression.height;
                    document.body.appendChild(pxRetargetingImpression);
                    rwmpVars.retargetingImpression = null;
                }
                if (data.hasOwnProperty('retargetingClick') && Object.keys(data.retargetingClick).length !== 0) {
                    rwmpVars.retargetingClick = data.retargetingClick;
                }
                if(!(Object.keys(data).length !== 0  && data.hasOwnProperty('medias')  && Object.keys(data.medias).length !== 0)) {
                    // Pas de médias, cherchons un VAST
                    if (data.hasOwnProperty('activity_id') && data.activity_id != '') {
                        rwmpVars.globalVast = 'https\u003A\/\/publishers.edisound.com\/api\/p\/medias\/vast/'+data.activity_id;
                    }
                    console.log('Missing sponsorized songs');
                    data = {"medias":[]};
                }
                rwmpLoadPlayer(data.medias);
            })
            .catch(
                (err) => {
                    console.log('Fetch https\u003A\/\/publishers.edisound.com\/api\/p\/medias\/fetch error : ', err);
                    console.log('An error occured. Your podcast will be displayed anyway');
                    rwmpLoadPlayer([]);
                }
            );
    }
        
